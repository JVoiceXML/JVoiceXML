<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false">
  <local-check-config name="JVoiceXML Checks" location="etc/jvoicexml-checks.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="src" enabled="true" check-config-name="JVoiceXML Checks" local="true">
    <file-match-pattern match-pattern="src" include-pattern="true"/>
    <file-match-pattern match-pattern="dist" include-pattern="false"/>
    <file-match-pattern match-pattern="doc" include-pattern="false"/>
    <file-match-pattern match-pattern="test/unitTests" include-pattern="true"/>
  </fileset>
  <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
