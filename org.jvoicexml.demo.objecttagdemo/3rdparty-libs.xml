<project name="Object Demo - 3rd party libraries" basedir=".">
  <description>
This file contains the 3rd party libary definitions, used by the Object Demo.
    </description>
  <property name="jvxml.core.dir" value="../org.jvoicexml" />
  <import file="${jvxml.core.dir}/3rdparty-libs.xml" />

  <target name="-local-3rdparty-setup" depends="-3rdparty-setup">
    <!-- local third party libraries -->
    <property name="local-3rdparty.dir" value="3rdparty" />

    <!-- jvxml library -->
    <path id="jvxml.core.lib">
        <path location="${jvxml.core.dir}/dist/jvxml-client.jar" />
        <path location="${jvxml.core.dir}/dist/jvxml-xml.jar" />
    </path>
  </target>
</project>
