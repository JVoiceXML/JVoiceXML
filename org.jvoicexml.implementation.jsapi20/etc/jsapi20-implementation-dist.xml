<?xml version="1.0" encoding="UTF-8"?>
<implementation xmlns:beans="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="jvxml-implementation-0-7.xsd">
  <repository>jsapi20</repository>
  <classpath>lib/jvxml-jsapi2.0.jar</classpath>
  <classpath>lib/jvxml-jsapi2.0-impl.jar</classpath>
  <classpath>lib/jsr-113.jar</classpath>
  <classpath>lib/jsr113-jsebase.jar</classpath>
  <classpath>lib/jsr113-jsebase-impl.jar</classpath>
  <classpath>lib/freetts/freetts.jar</classpath>
  <classpath>lib/freetts/cmu_us_kal.jar</classpath>
  <classpath>lib/freetts/cmulex.jar</classpath>
  <classpath>lib/freetts/en_us.jar</classpath>
  <classpath>lib/sphinx4.jar</classpath>
  <classpath>lib/WSJ_8gau_13dCep_16k_40mel_130Hz_6800Hz.jar</classpath>

  <beans:bean
    class="org.jvoicexml.implementation.jsapi20.jvxml.SynthesizedOutputFactory">
    <beans:property name="instances" value="1" />
  </beans:bean>

  <beans:bean class="org.jvoicexml.implementation.jsapi20.jvxml.SpokenInputFactory">
    <beans:property name="instances" value="1" />
  </beans:bean>
 
</implementation>
