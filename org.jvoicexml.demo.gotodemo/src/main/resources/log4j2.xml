<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout>
                <Pattern>%6r [%-20.20t] %-5p %30.30c (%6L) %x %m%n</Pattern>
             </PatternLayout>
        </Console>
    </Appenders>

    <Loggers>
        <Logger name="org.jvoicexml" level="debug">
        </Logger>

        <Root level="warn">
            <AppenderRef ref="stdout" />
        </Root>
    </Loggers>
</Configuration>
