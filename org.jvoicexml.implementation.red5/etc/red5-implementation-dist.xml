<?xml version="1.0" encoding="UTF-8"?>
<implementation xmlns:beans="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="jvxml-implementation-0-7.xsd">
  <repository>red5</repository>
  <classpath>lib/org.jvoicexml.implementation.red5.jar</classpath>

  <beans:bean
    class="org.jvoicexml.implementation.red5.Red5SynthesizedOutputFactory">
    <beans:property name="instances" value="1" />
  </beans:bean>

  <beans:bean class="org.jvoicexml.implementation.red5.Red5SpokenInputFactory">
    <beans:property name="instances" value="1" />
  </beans:bean>
 
</implementation>
