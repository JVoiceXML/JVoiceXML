<?xml version="1.0" encoding="UTF-8"?>
<implementation xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="jvxml-implementation-0-7.xsd">
    <repository>lightweightbml</repository>
    <classpath>dist/org.jvoicexml.implementation.lightweightbml.jar
    </classpath>

    <beans:bean
        class="org.jvoicexml.implementation.lightweightbml.BMLSynthesizedOutputFactory">
        <beans:property name="instances" value="1" />
        <beans:property name="type" value="bml" />
    </beans:bean>
</implementation>
