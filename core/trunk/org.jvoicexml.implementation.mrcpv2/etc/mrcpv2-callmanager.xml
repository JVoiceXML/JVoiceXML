<?xml version="1.0" encoding="UTF-8"?>
<callmanager xmlns:beans="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="jvxml-callmanager-0-7.xsd">
  <repository>mrcpv2</repository>
  <classpath>dist/jvxml-mrcpv2.jar</classpath>
  <classpath>dist/jvxml-client-mrcpv2.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/cairo-snapshot/lib/cairo-sip-0.2.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/cairo-snapshot/lib/cairo-client-SNAPSHOT.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/cairo-snapshot/lib/cairo-rtp-SNAPSHOT.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/jainsip1.2/lib/jain-sip-ri-1.2.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/jainsip1.2/lib/nist-sdp-1.0.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/jainsip1.2/lib/jain-sip-api-1.2.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/jainsip1.2/lib/concurrent-1.3.4.jar</classpath>  
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/mrcp4j/lib/mrcp4j-0.2.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/zanzibar-snapshot/lib/zanzibar-snapshot.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/speechcloud-snapshot/lib/speechcloud-client.jar</classpath>
   <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/httpcomponents4.0/lib/apache-mime4j-0.5.jar</classpath>  
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/httpcomponents4.0/lib/httpclient-4.0-beta2.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/httpcomponents4.0/lib/httpcore-4.0.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/httpcomponents4.0/lib/httpmime-4.0-beta2.jar</classpath>
   <classpath>../org.jvoicexml.implementation.mrcpv2/3rdparty/commons-lang2.3/lib/commons-lang-2.3.jar</classpath>
  <classpath>../org.jvoicexml.implementation.mrcpv2/3rdpartyjsgf/lib/org.jvoicexml.implementation.jsapi10.grammar.jar</classpath>
  

  <beans:bean id="callmanager"
    class="org.jvoicexml.callmanager.mrcpv2.SipCallManager"
    init-method="startup" 
    destroy-method="shutdown">
    <beans:property name="remoteClientFactory">
      <beans:bean class="org.jvoicexml.callmanager.mrcpv2.Mrcpv2RemoteClientFactory">
      </beans:bean>
    </beans:property>
    <beans:property name="sipServer">
       <beans:ref bean="sipServer"/>
    </beans:property>
    <beans:property name="applications">
      <beans:list>
        <beans:bean
          class="org.jvoicexml.callmanager.ConfiguredApplication">
          <beans:property name="terminal"
            value="@sip.terminal@" />
          <beans:property name="uri"
            value="http://127.0.0.1:8080/helloworldservletdemo/HelloWorld" />
          <beans:property name="inputType" value="@sip.inputType@" />
          <beans:property name="outputType" value="@sip.outputType@" />
        </beans:bean>
      </beans:list>
    </beans:property>
    <beans:property name="cloudUrl">
       <beans:value>http://spokentech.net/speechcloud</beans:value> 
     </beans:property>
  </beans:bean>
  <beans:bean id="sipServer" class="org.speechforge.zanzibar.sip.SipServer"
    init-method="startup" destroy-method="shutdown">
    <beans:property name="dialogService">
       <beans:ref bean="callmanager"/>
    </beans:property>
    <beans:property name="mySipAddress">
      <beans:value>@mrcpv2.sip.address@</beans:value>
    </beans:property>
    <beans:property name="stackName">
      <beans:value>Mrcpv2SessionManager</beans:value>
    </beans:property>
    <beans:property name="port">
      <beans:value>@mrcpv2.sip.port@</beans:value>
    </beans:property>
    <beans:property name="transport">
      <beans:value>UDP</beans:value>
    </beans:property>
    <beans:property name="cairoSipAddress">
      <beans:value>@mrcpv2.sip.cairo.sip.address@</beans:value>
    </beans:property>
    <beans:property name="cairoSipHostName">
      <beans:value>@mrcpv2.sip.cairo.sip.host@</beans:value>
    </beans:property>
    <beans:property name="cairoSipPort">
      <beans:value>@mrcpv2.sip.cairo.sip.port@</beans:value>
    </beans:property>  
    <beans:property name="mode">
		<beans:value>cloud</beans:value>
	</beans:property>
	<beans:property name="baseReceiverRtpPort">
		<beans:value>42150</beans:value>
	</beans:property>
	<beans:property name="baseXmitRtpPort">
		<beans:value>42050</beans:value>
	</beans:property>
	<beans:property name="maxConnects">
		<beans:value>50</beans:value>
	</beans:property>

  </beans:bean>
</callmanager>
